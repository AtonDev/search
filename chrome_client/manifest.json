{
  "manifest_version": 2,

  "name": "Alt S",
  "description": "An alternative way to search the web.",
  "version": "0.1",

  "permissions": [
     "http://*/*",
     "https://*/*",
     "tabs",
     "activeTab"
  ],
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["headroom.js", "jquery-1.11.1.min.js", "content_script.js"],
      "run_at": "document_start"
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "toolbarInitial.html"
  },

  "web_accessible_resources": [
    "css/*.css",
    "toolbar.html",
    "toolbarInitial.html",
    "autocomplete.html"
  ],

  "omnibox": { "keyword" : "ÃŸ" },

  "commands": {
    "_execute_browser_action" : {
      "suggested_key": { "default": "Alt+S" }
    }
  }
}
